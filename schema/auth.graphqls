input SignUpInput {
    organizationName: String!
    userName: String!
    userEmail: String!
    userPassword: String!
}

input LogInInput {
    userEmail: String!
    userPassword: String!
}

extend type Query {
    authToken: String!
}

extend type Mutation {
    signUp(input: SignUpInput!): String!
    logIn(input: LogInInput!): String!
    logOut: Boolean
}
